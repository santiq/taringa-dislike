{
  "manifest_version": 2,

  "name": "Taringa! shouts dislike",
  "short_name": "T! shouts",
  "description": "Vota negativamente los shouts que no te gustan!",
  "version": "1.1.0",
  
  "content_scripts": [
      {
          "matches": ["http://www.taringa.net/*","https://www.taringa.net/*"],
          "js": ["jquery.js","content.js"]
      }
  ],
  "icons":{
    "16":"icons/icon16.png",
    "32":"icons/icon32.png",
    "48":"icons/icon48.png",
    "64":"icons/icon64.png",
    "96":"icons/icon96.png",
    "128":"icons/icon128.png"
  },
   "permissions": [
    "http://*.herokuapp.com/*"
  ]
}